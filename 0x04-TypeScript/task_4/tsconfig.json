{
  "compilerOptions": {
    "outDir": "./js",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES2020",
    "target": "ES2020",
    "allowJs": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}

